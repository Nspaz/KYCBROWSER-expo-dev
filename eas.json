{
  "cli": {
    "version": ">= 10.0.0",
    "requireCommit": true
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "development-device": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "sideload": {
      "distribution": "internal",
      "ios": {
        "buildConfiguration": "Release"
      }
    },
    "enterprise-ios": {
      "distribution": "internal",
      "ios": {
        "buildConfiguration": "Release"
      },
      "env": {
        "RNC_ENTERPRISE_WEBKIT": "1"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "YOUR_APPLE_ID_EMAIL_HERE",
        "ascAppId": "YOUR_APP_STORE_CONNECT_APP_ID_HERE"
      },
      "android": {
        "track": "internal"
      }
    }
  }
}
